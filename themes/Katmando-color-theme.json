{
	"name": "Katmando",
	"colors": {
		"editor.foreground": "#eeffff",
    "editor.background": "#090909",
    "editor.selectionBackground": "#333",
    "editorBracketHighlight.foreground1": "#eeffff",
    "editorBracketHighlight.foreground2": "#eeffff",
    "editorBracketHighlight.foreground3": "#eeffff",
    "editorBracketHighlight.foreground4": "#eeffff",
    "editorBracketHighlight.foreground5": "#eeffff",
    "editorBracketHighlight.foreground6": "#eeffff",
    "editorBracketMatch.border": "#ee5",
    "editorGroupHeader.border": "#444444",
    "editorSuggestWidget.selectedBackground": "#000000",
    "editorUnnecessaryCode.border": "#ffffff50",
    "editorUnnecessaryCode.opacity": "#000f",
    "panel.border": "#444444",
    "quickInputList.focusBackground": "#000000",
    "selection.background": "#333",
    "sideBar.background": "#090909",
    "sideBar.border": "#444444",
    "statusBar.background": "#111",
    "statusBar.noFolderBackground": "#111",
    "titleBar.activeBackground": "#111",
	},
	"tokenColors": [{
    "name": "comments",
    "scope": [
      "comment",
      "comment.block",
      "comment.block.documentation",
      "comment.line",
    ],
    "settings": {
      "fontStyle": "italic",
      "foreground": "#666666",
    }
  }, {
    "name": "keywords",
    "scope": [
      "entity.name.tag",
      "entity.other.attribute-name.class.pug",
      "keyword.control.as.js",
      "keyword.control.as.ts",
      "keyword.control.conditional.js",
      "keyword.control.conditional.ts",
      "keyword.control.default.js",
      "keyword.control.default.ts",
      "keyword.control.export.js",
      "keyword.control.export.ts",
      "keyword.control.flow.js",
      "keyword.control.flow.ts",
      "keyword.control.from.js",
      "keyword.control.from.ts",
      "keyword.control.import.js",
      "keyword.control.import.ts",
      "keyword.control.lua",
      "keyword.local.lua",
      "keyword.operator.expression.in.js",
      "keyword.operator.expression.in.ts",
      "keyword.operator.new.js",
      "keyword.operator.new.ts",
      "punctuation.definition.template-expression.begin.js",
      "punctuation.definition.template-expression.begin.ts",
      "punctuation.definition.template-expression.end.js",
      "punctuation.definition.template-expression.end.ts",
      "punctuation.separator.key-value.html",
      "punctuation.separator.key-value.js",
      "punctuation.separator.key-value.ts",
      "storage.modifier.async.js",
      "storage.modifier.async.ts",
      "storage.modifier.js",
      "storage.modifier.ts",
      "storage.type",
      "storage.type.js",
      "storage.type.ts",
      "storage.type.function.arrow.js",
      "storage.type.function.arrow.ts",
      "variable.language.this.js",
      "variable.language.this.ts",
    ],
    "settings": {
      "foreground": "#777",
      "fontStyle": "italic"
    }
  }, {
    "name": "functions",
    "scope": [
      "entity.name.function.js",
      "entity.name.function.lua",
      "entity.name.function.ts",
    ],
    "settings": {
      "foreground": "#ee5",
    }
  }]
}
